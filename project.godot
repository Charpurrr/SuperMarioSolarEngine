; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SolarEngine"
config/tags=PackedStringArray("mario_fangames")
run/main_scene="res://areas/world_machine.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=60
boot_splash/bg_color=Color(0.513726, 0.403922, 0.470588, 1)
boot_splash/image="res://logo.png"
boot_splash/use_filter=false

[autoload]

LocalSettings="*res://util/globals/local_settings.gd"
GameState="*res://util/globals/game_state.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/mode=2
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"
mouse_cursor/custom_image="res://level_designer/tools/select_tool/select.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/button_property/plugin.cfg", "res://addons/rmsmartshape/plugin.cfg")

[file_customization]

folder_colors={
"res://actors/": "blue",
"res://actors/player/luigi/": "green",
"res://actors/player/mario/": "red",
"res://areas/": "green",
"res://level_designer/": "teal",
"res://props/": "pink",
"res://ui/": "orange",
"res://util/": "purple"
}

[filesystem]

import/blender/enabled=false

