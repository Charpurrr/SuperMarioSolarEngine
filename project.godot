; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Solar Engine"
config/version="0.1.x"
config/tags=PackedStringArray("mario_fangames")
run/main_scene="res://ui/key_scenes/title_screen/title_screen.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
run/max_fps=60
boot_splash/bg_color=Color(0.0862745, 0.0862745, 0.0862745, 1)
boot_splash/use_filter=false
boot_splash/image="res://splash.png"
config/icon="res://icon.png"

[autoload]

LocalSettings="*res://util/globals/local_settings.gd"
GameState="*res://util/globals/game_state.gd"
MusicManager="*res://util/globals/music_manager.tscn"
TransitionManager="*res://util/globals/transition_manager.tscn"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"
window/vsync/vsync_mode=0
mouse_cursor/custom_image="uid://b6gpj074qmkmk"

[editor_plugins]

enabled=PackedStringArray("res://addons/rmsmartshape/plugin.cfg")

[file_customization]

folder_colors={
"res://entities/": "blue",
"res://entities/player/luigi/": "green",
"res://entities/player/mario/": "red",
"res://level_designer/": "teal",
"res://props/": "pink",
"res://stages/": "green",
"res://ui/": "orange",
"res://util/": "purple"
}

